{
    "$id": "driver.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Racing Driver",
    "type": "object",
    "properties": {
      "name": { "type": "string"},
      "nationality": { "type": "string"},
      "team": { "type": "string" },
      "wins": { "type": "integer", "minimum": 0 }
    },
    "required": ["name", "nationality", "team", "wins"]
  }
  